<h4>Products Listing</h4>

<table class="bordered highlight centered">
  <thead>
    <tr>
      <th>Item Name</th>
      <th>Unit Price</th>
    </tr>
  </thead>
  <tbody>
    <% @products["prices"].each do |item| %>
    <tr>
      <td><%= item["name"] %></td>
      <td><%= item["unit_price"] %></td>
    </tr>
    <% end %>
  </tbody>
</table>



